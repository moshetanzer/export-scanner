{
  "name": "all-exports",
  "version": "1.0.0",
  "packageManager": "pnpm@10.11.0",
  "description": "",
  "author": "",
  "license": "ISC",
  "keywords": [],
  "sideEffects": false,
  "exports": {
    ".": "./dist/index.mjs"
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/index.d.mts"
      ]
    }
  },
  "scripts": {
    "build": "unbuild",
    "dev": "vitest dev",
    "lint": "eslint . --fix",
    "prepare": "simple-git-hooks",
    "prepack": "pnpm build",
    "prepublishOnly": "pnpm lint && pnpm test",
    "release": "bumpp && pnpm publish",
    "test": "pnpm test:unit && pnpm test:types",
    "test:unit": "vitest",
    "test:knip": "knip",
    "test:versions": "installed-check -d --no-workspaces",
    "test:types": "tsc --noEmit"
  },
  "dependencies": {
    "lodash": "^4.17.21",
    "pkg-exports": "^0.2.0",
    "ramda": "^0.30.1",
    "scule": "^1.3.0",
    "text-toolbox": "^1.0.15",
    "three": "^0.176.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "latest",
    "@types/figlet": "^1.7.0",
    "@types/inquirer": "^9.0.8",
    "@types/minimist": "^1.2.5",
    "@types/node": "^22.15.21",
    "@vitest/coverage-v8": "latest",
    "bumpp": "latest",
    "changelogithub": "latest",
    "eslint": "latest",
    "installed-check": "latest",
    "knip": "latest",
    "lint-staged": "latest",
    "pkg-pr-new": "^0.0.50",
    "simple-git-hooks": "latest",
    "typescript": "latest",
    "unbuild": "latest",
    "vitest": "latest"
  },
  "resolutions": {
    "package-name": "link:."
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  },
  "lint-staged": {
    "*.{js,ts,mjs,cjs,json,.*rc}": [
      "npx eslint --fix"
    ]
  }
}
